{
  "name": "singlelineindent",
  "displayName": "singleLineIndent",
  "description": "Indent a single line whenever a char is selected.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "author": "7hebel",
  "publisher": "gental-py",
  "repository": {
    "type": "git",
    "url": "https://github.com/7hebel/vscode-ext-indent-line"
  },
  "contributes": {
    "commands": [
      {
        "command": "singlelineindent.indentLine",
        "title": "Indent Line on Tab"
      }
    ],
    "keybindings": [
      {
        "command": "singlelineindent.indentLine",
        "key": "tab",
        "when": "editorHasSelection && editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}
